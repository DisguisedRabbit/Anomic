function Speedometer()
	if game.CoreGui:FindFirstChild('Speedometer') then
    	game.CoreGui:FindFirstChild('Speedometer'):Destroy()
    end
local Speedometer = Instance.new("ScreenGui")
local Speed = Instance.new("TextLabel")
Speedometer.Name = "Speedometer"
Speedometer.Parent = game.CoreGui
Speedometer.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Speed.Name = "Speed"
Speed.Parent = Speedometer
Speed.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Speed.BackgroundTransparency = 1.000
Speed.Position = UDim2.new(0.579770565, -177, 0.961330652, 0)
Speed.Size = UDim2.new(0, 146, 0, 19)
Speed.Font = Enum.Font.Highway
Speed.Text = "Speed: 0"
Speed.TextColor3 = Color3.fromRGB(0, 0, 0)
Speed.TextSize = 19.000
Speed.TextXAlignment = "Left"
Speed.TextYAlignment = "Top"
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	Speed.Position =
		UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end
Speed.InputBegan:Connect(
	function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = Speed.Position

			input.Changed:Connect(
				function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end
			)
		end
	end
)
Speed.InputChanged:Connect(
	function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end
)
UIS.InputChanged:Connect(
	function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end
)
repeat wait()
Speed.Text = ("Speed: "..math.floor(game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity.Magnitude))
until game.Players.LocalPlayer.Character.Humanoid.Health <= 0 or Speed == nil
if game.Players.LocalPlayer.Character.Humanoid.Health <= 0 then
Speedometer:Destroy()
end
end
--UIS
UIS.InputBegan:Connect(function(input, gameProcessedEvent)
	if input.UserInputType == Enum.UserInputType.Keyboard then
		if input.KeyCode == Enum.KeyCode.Equals then
			FLYING = not FLYING
			if FLYING then
				startFly()               
			else
				stopFly()             
			end
		end
	end
end)
FREECAM = false
UIS.InputBegan:Connect(function(input, gameProcessedEvent)
	if input.UserInputType == Enum.UserInputType.Keyboard then
		if input.KeyCode == Enum.KeyCode.RightBracket then
			FREECAM = not FREECAM
			if FREECAM then
				StartFreecam()               
			else
				StopFreecam()             
			end
		end
	end
end)
UIS.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.UserInputType == Enum.UserInputType.Keyboard then
        if input.KeyCode == Enum.KeyCode.Q then
            if mouse.Target then 
                if currentVehicle ~= nil then
                    currentVehicle:SetPrimaryPartCFrame(CFrame.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z) * CFrame.new(0,-2,0))
                else 
                LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z)      
                end
            end
		end
	end
end)
NOCLIPPING = false
UIS.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.UserInputType == Enum.UserInputType.Keyboard then
        if input.KeyCode == Enum.KeyCode.Minus then
            NOCLIPPING = not NOCLIPPING
			if NOCLIPPING then
			Noclipping = game:GetService('RunService').Stepped:Connect(noclip)	
			else
			Noclipping:Disconnect()
			end
		end
	end
end)
UIS.InputBegan:connect(function(key)
    if key.KeyCode == Enum.KeyCode.LeftShift then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 23
    end
end)
UIS.InputEnded:connect(function(key)
    if key.KeyCode == Enum.KeyCode.LeftShift then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 13
    end
end)
UIS.InputBegan:connect(function(key)
    if key.KeyCode == Enum.KeyCode.M then
		LPlayer.PlayerGui.MainUIHolder.GameMap.Visible = true
    end
end)
UIS.InputEnded:connect(function(key)
    if key.KeyCode == Enum.KeyCode.M then
        LPlayer.PlayerGui.MainUIHolder.GameMap.Visible = false
    end
end)
UIS.InputBegan:connect(function(UserInput)
    if UserInput.UserInputType == Enum.UserInputType.Keyboard and UserInput.KeyCode == Enum.KeyCode.Space then
        Action(LPlayer.Character.Humanoid, function(self)
            if self:GetState() == Enum.HumanoidStateType.Jumping or self:GetState() == Enum.HumanoidStateType.Freefall then
                Action(self.Parent.HumanoidRootPart, function(self)
                    self.Velocity = Vector3.new(0, 30, 0);
                end)
            end
        end)
    end      
end)
UIS.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.UserInputType == Enum.UserInputType.Keyboard then
        if input.KeyCode == Enum.KeyCode.LeftAlt then
            Main:toggle()
		end
	end
end) 
