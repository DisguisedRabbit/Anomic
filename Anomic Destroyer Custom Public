local currentTool = nil 
local color1 = Color3.fromRGB(0,0,0)
local color2 = Color3.fromRGB(0,0,0)
paintSection:addColorPicker("Primary Color", Color3.fromRGB(0,0,0), function(c)
    color1 = c
end)
paintSection:addColorPicker("Secondary Color", Color3.fromRGB(0,0,0), function(c)
    color2 = c
end)
paintSection:addButton("Paint Item", function()
    for i,v in pairs(LPlayer.Character:GetChildren()) do
        if v:IsA("Tool") and v ~= nil then  
            currentTool = v
        end
    end
    game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PaintTool:FireServer(currentTool,color1,color2)
end)
paintSection:addButton("Paint Joker", function()
    for i,v in pairs(LPlayer.Character:GetChildren()) do
        if v:IsA("Tool") and v ~= nil then  
            currentTool = v
            color1 = Color3.fromRGB(0,255,0)
            color2 = Color3.fromRGB(140,0,255)
        end
    end
    game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PaintTool:FireServer(currentTool,color1,color2)
    color1 = Color3.fromRGB(0,0,0)
    color2 = Color3.fromRGB(0,0,0)
end)
paintSection:addButton("Paint JigglyPuff", function()
    for i,v in pairs(LPlayer.Character:GetChildren()) do
        if v:IsA("Tool") and v ~= nil then  
            currentTool = v
            color1 = Color3.fromRGB(248,158,252)
            color2 = Color3.fromRGB(223,120,214)
        end
    end
    game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PaintTool:FireServer(currentTool,color1,color2)
    color1 = Color3.fromRGB(0,0,0)
    color2 = Color3.fromRGB(0,0,0)
end)
paintSection:addButton("Paint SpiderMan", function()
    for i,v in pairs(LPlayer.Character:GetChildren()) do
        if v:IsA("Tool") and v ~= nil then  
            currentTool = v
            color1 = Color3.fromRGB(0,0,255)
            color2 = Color3.fromRGB(255,0,0)
        end
    end
    game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PaintTool:FireServer(currentTool,color1,color2)
    color1 = Color3.fromRGB(0,0,0)
    color2 = Color3.fromRGB(0,0,0)
end)

local buyAmmoAmount = 1
local ammoType = ""
otherSection:addDropdown("Ammo", {"9mm", "5.56", "12 Gauge", ".50", ".45 ACP", "5.7x28"}, function(valuex)
    ammoType = valuex
end)
otherSection:addSlider("Ammo Amount", 1, 0, 10000, function(v)
    buyAmmoAmount = v
end)
otherSection:addButton("Buy ammo", function()
    for i = 1, buyAmmoAmount, 1 do
        wait(0.1)
        game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PurchaseTeamItem:FireServer(ammoType,"Single",nil)
        
    end
end)
local weaponType = nil  
otherSection:addDropdown("Weapon type", {"Sawed Off", "Micro SMG", "Light Vest", "9mm", "AR", "PDW .45", "Heavy Pistol", "Service Rifle", "Skorpion", "Tactical SMG", "Shotgun", "Bullpup Shotgun", "Handgun", "Revolver", "Snubnose", "Lockpick"}, function(t)
    weaponType = t
end)
otherSection:addButton("Buy Weapon", function()
    if weaponType ~= nil then
        if isCrate then
            game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PurchaseTeamItem:FireServer(weaponType,"Crate",nil)
            game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").DeliveryFunction:FireServer("PickUpDelivery",weaponType)
        else
            game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").PurchaseTeamItem:FireServer(weaponType,"Single",nil)
        end
    end
end)

miscSection:addButton("Rejoin", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game:GetService("Players").LocalPlayer)
end)
miscSection:addButton("Reset to 50k", function()
    for i,v in pairs(workspace.PlayerVehicles:GetChildren()) do
        game:GetService("ReplicatedStorage")["_CS.Events"].FillUpCar:FireServer(v, 9e9)
    end
    wait(.2)
    game:GetService("TeleportService"):Teleport(game.PlaceId)
end)

miscSection:addButton("Bring all cars", function() 
    oldCFrame = LPlayer.Character.HumanoidRootPart.CFrame
    for i,v in pairs(game:GetService("Workspace").PlayerVehicles:GetChildren()) do
        if v:FindFirstChild("VehicleSeat") and v ~= nil and v:FindFirstChild("VehicleSeat").Damage.Value > 1 and not v:FindFirstChild("VehicleSeat"):FindFirstChild("SeatWeld") then
            local Carseat = v:FindFirstChild("VehicleSeat")        
            Carseat.Disabled = false      
            wait(1)   
            if Carseat ~= nil and Carseat then
                if Carseat.Parent:FindFirstChild("VehicleSeat") and not Carseat:FindFirstChild("SeatWeld") then   
                    Carseat.Disabled = false        
                    for i = 20, 0, -1 do        
                        wait(.15)
                        LPlayer.Character.HumanoidRootPart.CFrame = Carseat.CFrame           
                    end        
                    wait()  
                    if LPlayer.Character.Humanoid.SeatPart ~= nil then      
                        LPlayer.Character.Humanoid.SeatPart.Parent:SetPrimaryPartCFrame(oldCFrame * CFrame.new(0,2,0))
                        wait(1)
                        LPlayer.Character:FindFirstChild("Humanoid").Sit = false
                        wait()
                        LPlayer.Character:FindFirstChildOfClass("Humanoid").Jump = true
                    else
                        LPlayer.Character.HumanoidRootPart.CFrame = oldCFrame
                    end                            
                end
            end   
        end
    end
end)
miscSection:addButton("Crash passengers", function() 
    seat = game.Players.LocalPlayer.Character.Humanoid.SeatPart
    seat.Parent:MoveTo(Vector3.new(seat.Parent.PrimaryPart.Position.X, workspace.FallenPartsDestroyHeight+1, seat.Parent.PrimaryPart.Position.Z))
end)
miscSection:addButton("Skydive passengers", function() 
    seat = game.Players.LocalPlayer.Character.Humanoid.SeatPart
    seat.Parent:MoveTo(Vector3.new(seat.Parent.PrimaryPart.Position.X, seat.Parent.PrimaryPart.Position.Y + 30000, seat.Parent.PrimaryPart.Position.Z))
end)
miscSection:addButton("Spawn Car", function() 
    CSEvents.SpawnVehicle:FireServer(LPlayer.Character.HumanoidRootPart.CFrame, LPlayer.Character:FindFirstChildWhichIsA("Tool"));
end)
miscSection:addButton("Break Car", function() 
    for _,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			for i,c in pairs(v:GetDescendants()) do
                if c.Name == "Damage" or c.Name == "Fuel" then
                 c:Destroy()
                     c:Destroy()
                    end
				end
			end
		end
end)
miscSection:addButton("Alarm Cars", function() 
	for i,v in pairs(workspace.PlayerVehicles:GetChildren()) do
		local mag = (v:FindFirstChild('Engine').Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
   		if mag <= 10000 then
			game.Players.LocalPlayer.Backpack:FindFirstChildWhichIsA("Tool").Parent = game.Players.LocalPlayer.Character
			game:GetService("ReplicatedStorage"):FindFirstChild("_CS.Events").ToolEvent:FireServer("TriggerCarAlarm", v, game:GetService("Players").LocalPlayer.Character:FindFirstChildWhichIsA('Tool'))
			game.Players.LocalPlayer.Character:FindFirstChildWhichIsA('Tool').Parent = game.Players.LocalPlayer.Backpack
    	end
	end
end)
Debug:addButton("Dex Explorer", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/DisguisedRabbit/Anomic/main/Dex%20Explorer"))();
end)
Debug:addButton("Simple Spy", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/DisguisedRabbit/Anomic/main/Simply%20Spy"))();
end)
Debug:addButton("Btools", function()
	Instance.new("HopperBin", LPlayer:FindFirstChildOfClass("Backpack")).BinType = 1
	Instance.new("HopperBin", LPlayer:FindFirstChildOfClass("Backpack")).BinType = 2
	Instance.new("HopperBin", LPlayer:FindFirstChildOfClass("Backpack")).BinType = 3
	Instance.new("HopperBin", LPlayer:FindFirstChildOfClass("Backpack")).BinType = 4
end)
bypass()
setTheme()
local GC = getconnections or get_signal_cons
	if GC then
		for i,v in pairs(GC(Players.LocalPlayer.Idled)) do
			if v["Disable"] then
				v["Disable"](v)
			elseif v["Disconnect"] then
				v["Disconnect"](v)
			end
		end
	end
LPlayer.CharacterAdded:Connect(function() 
    wait(1.5)	   
    setTheme()
	wait(2)
	bypass()
end)
Main:SelectPage(Main.pages[1], true)
